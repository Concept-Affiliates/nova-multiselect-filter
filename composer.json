{
  "name": "optimistdigital/nova-multiselect-filter",
  "description": "A Laravel Nova filter.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "laravel/nova": "^2.0.11 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigtal\\NovaMultiselectFilter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigtal\\NovaMultiselectFilter\\FilterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
